ブログ一覧画面

<div class="container">
	<div class="col-xs-4"></div>
	<div class="col-xs-5">
		<% @blogs.each do |blog| %>
			<table class="table table-hover">
				<tbody>
					<h4 class="blog-title"><%= link_to blog.title, blog_path(blog.id) %></h4>
					<p class="blog-updatetime"><%= blog.created_at %></p>
				</tbody>
			</table>
		<% end %>
	</div>
</div>